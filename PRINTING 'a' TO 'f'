;PRINTING 'a' TO 'f'

.MODEL SMALL
.DATA
X DB 'a'
.CODE
MOV AX,@DATA
MOV DS,AX

MOV AH,02H
MOV DL,X
INT 21H
MOV DL,X
MOV CX,5    ;INCREMENT REGISTER CX , THE VALUE OF DL REGISTER IS INCREMENT 5 TIMES

LABEL1:
    INC DL  ;INC: INCREMENT REDISTER, DL INCREMENT BY 1 
    INT 21H ;INTURRUPT HANDLER
LOOP LABEL1

MOV AH,4CH
INT 21H

END
